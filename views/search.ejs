    <form action="/search" method="get" class="mainform" id="form" >
    <div class="input-group mb-3">
      <button class="btn btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false"><svg xmlns="http://www.w3.org/2000/svg" width="23" height="25" fill="currentColor" class="bi bi-geo-alt-fill" viewBox="0 0 16 16">
        <path d="M8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10zm0-7a3 3 0 1 1 0-6 3 3 0 0 1 0 6z"/>
      </svg>&nbsp;</button>
      <ul class="dropdown-menu">
        <li><a class="dropdown-item" href="#">Bengaluru</a></li>
        <li><a class="dropdown-item" href="#">Delhi</a></li>
        <li><a class="dropdown-item" href="#">Noida</a></li>
        <li><a class="dropdown-item" href="#">Mathura</a></li>
      </ul>
      <input type="text" class="form-control long-input" id="locationinp"  name="location" aria-label="Text input with dropdown button"
      placeholder="Select Location" value="Cityh" required>
    </div>
    <br><br>  
      <div class="pickup">
      <label for="pickupDate">Pickup Date :</label><br>
      <input class="niche" type="date" id="pickupDate" name="pickupDate" value="2023-11-30" required><br>
      <label for="pickupTime">Pickup Time : </label><br>
      <input class="niche" type="time" id="pickupTime" name="pickupTime" value="11:50" required><br><br>
    </div>
     

<br>
  <div class="pickup">
  <label for="dropDate">Drop Date :</label><br>
  <input class="niche" type="date" id="dropDate" name="dropDate" value="2023-12-01" required><br>
  <label for="dropTime">Drop Time :</label><br>
  <input class="niche" type="time" id="dropTime" name="dropTime" value="16:04" required><br><br>
</div>
<br><br><br>
<input class="submit-button" type="submit" value="Search">   
</form> 
    
  </div>

<!-- description box right side vala -->

  <div class="description-box">

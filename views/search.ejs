<!-- views/cars.ejs -->

<h2 class="explore">Explore Our Cars...</h2>
<br>
<div class="car-grid">
  <% cars.slice(0,6).forEach((car,index) => { %>
    <!-- Car Item -->
    <div class="car-item">
      <img class="car-image" src="<%= car.img %>" alt="<%= car.name %>">
